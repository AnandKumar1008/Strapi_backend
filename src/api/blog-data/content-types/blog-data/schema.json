{
  "kind": "collectionType",
  "collectionName": "blog_datas",
  "info": {
    "singularName": "blog-data",
    "pluralName": "blog-datas",
    "displayName": "Blog_Data",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "blocks",
      "required": true
    },
    "Blog_Videos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Infographics": {
      "type": "component",
      "repeatable": false,
      "component": "infographics.infographics"
    },
    "Additional_Fields": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "BlogDetail": {
      "type": "component",
      "repeatable": true,
      "component": "blog-data.blog-details"
    },
    "SEO": {
      "type": "component",
      "repeatable": true,
      "component": "shared.seo"
    }
  }
}
